{
    "architecture": "@CLICK_ARCH@",
    "description": "Listen to your favourite tracks on Soundcloud",
    "framework": "ubuntu-sdk-14.10-dev2",
    "hooks": {
        "soundcloud": {
            "apparmor": "soundcloud.apparmor",
            "scope": "soundcloud",
            "account-application": "soundcloud.application",
            "account-service": "soundcloud.service"
        },
        "accounts": {
            "account-provider": "soundcloud.provider",
            "account-qml-plugin": "qml-plugin"
        }
    },
    "icon": "soundcloud/icon.png",
    "maintainer": "Ubuntu Developers <ubuntu-devel-discuss@lists.ubuntu.com>",
    "name": "com.ubuntu.scopes.soundcloud",
    "title": "Soundcloud scope",
    "version": "0.1"
}
